#include <bits/stdc++.h> 
#include <iostream>
using namespace std;

int main() {
    int num,power;
    cin>>num>>power;
    int ans =1;
    for(int i=1; i<=power; i++){
        ans = ans * num; 
    }
    cout<<ans;
   

    return 0;
}
--------------------------------------------------------------------------------------
#include <bits/stdc++.h> 
#include <iostream>
#include <cmath>

using namespace std;

int main() {
    int x,n;
    cin>>x>>n;
    int ans = pow(x,n);
    cout<<ans;
    return 0;
}
------------------------------------------------------------------------------------------

#include <bits/stdc++.h> 
#include <iostream>
using namespace std;
int solve(int n, int x){
    if(n==0){
        return 1;
    }
    int chotproblem = solve(n-1, x);
    int badproblem = x * chotproblem;
    return badproblem;
}
int main() {
    int x,n;
    cin>>x>>n;

    int ans = solve(n,x);
    cout<<ans;
    return 0;
}
