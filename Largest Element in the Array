#include <bits/stdc++.h> 
int largestElement(vector<int> &arr, int n) {
    int ans = 0;
    for(int i = 0; i<arr.size(); i++){
        if(arr[i] >= ans){
            ans = arr[i];
        }
    }
    return ans;
}

----------------------------------------------------------

#include <bits/stdc++.h> 
int largestElement(vector<int> &arr, int n) {
    sort(arr.begin(), arr.end());
    return arr[n-1];
}
