tC=O(N)
SC= O(1)

class Solution {
public:
    void reverseString(vector<char>& s) {
        reverse(s.begin(), s.end());
    }
};

--------------------------------------------------------------------------------------------------------
TC=O(N)
SC= O(1)

class Solution {
public:
    void reverseString(vector<char>& s) {
       int n = s.size(); 
       int start = 0, end = n-1;

       while(start <= end){
            swap(s[start], s[end]);
            start++;
            end--;
       } 
    }
};
