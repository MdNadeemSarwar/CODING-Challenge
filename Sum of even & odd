#include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    int odd, even = 0;
    while(n != 0){
        int digit = n%10;
        if(digit % 2 == 0){
            even = even + digit;
        }
        else{
            odd = odd + digit;
        }
        n = n/10;
    }
    cout<<even<<" "<<odd;

    return 0;
}
